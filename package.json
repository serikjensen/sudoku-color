{
  "name": "sudokucolor",
  "version": "1.0.0",
  "description": "Colorized sudoku app",
  "main": "client/index.js",
  "repository": "git@github.com:serikjensen/SudokuColor.git",
  "author": "serikjensen",
  "license": "MIT",
  "sideEffects": false,
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.prod.js",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "tableflip": "rm yarn.lock && rm -rf node_modules/ && yarn cache clean && yarn install",
    "test": "./node_modules/karma/bin/karma start karma.conf.js --single-run --mode=production",
    "test:dev": "./node_modules/karma/bin/karma start karma.conf.js --single-run=false --mode=development",
    "lint": "eslint ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
